   "source": [
    "import numpy as np\n",
    "from scipy.special import erfc\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "SNRdB=np.arange(0,10)\n",
    "SNR= 10**(SNRdB/10)\n",
    "BER_TH= 0.5*erfc(np.sqrt(SNR))\n",
    "\n",
    "plt.semilogy(SNRdB, BER_TH, 'r')\n",
    "plt.show()\n"
   ]
